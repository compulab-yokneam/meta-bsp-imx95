From: Valentin Raevsky <valentin@compulab.co.il>
Date: Sat, 16 Nov 2024 22:29:32 +0200
Subject: [PATCH 9/9] debug: ucm-imx95: dts: Update bluetooth compatible string

Signed-off-by: Valentin Raevsky <valentin@compulab.co.il>
---
 arch/arm64/boot/dts/compulab/ucm-imx95-headless.dts | 3 +--
 1 file changed, 1 insertion(+), 2 deletions(-)

diff --git a/arch/arm64/boot/dts/compulab/ucm-imx95-headless.dts b/arch/arm64/boot/dts/compulab/ucm-imx95-headless.dts
index 9b925ec5f533..80430d5fb77e 100644
--- a/arch/arm64/boot/dts/compulab/ucm-imx95-headless.dts
+++ b/arch/arm64/boot/dts/compulab/ucm-imx95-headless.dts
@@ -468,9 +468,8 @@ &lpuart5 {
 	pinctrl-names = "default";
 	pinctrl-0 = <&pinctrl_uart5>;
 	status = "okay";
-
 	bluetooth {
-		compatible = "nxp,88w8987-bt";
+		compatible = "nxp,88w8997-bt";
 		/* U-blox 88Q9098 module use the 3Mbps by default */
 		fw-init-baudrate = <3000000>;
 	};
